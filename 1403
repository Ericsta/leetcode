class Solution:
    def minSubsequence(self, nums: List[int]) -> List[int]:
        # æ±‚å’Œ n 
        # æ’åº nlogif
        sum_a=sum(nums)
        # for i in nums:
        #     sum+=i
        a = sorted(nums,reverse=True)
        ans=[]
        result=0
        for i in a:
            result+=i
            ans.append(i)
            if result>(sum_a/2):
                return ans
