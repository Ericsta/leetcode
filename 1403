class Solution:
    def minSubsequence(self, nums: List[int]) -> List[int]:
        # 求和 n 
        # 排序 nlogif
        sum=sum(nums)
        # for i in nums:
        #     sum+=i
        a = sorted(nums,reverse=True)
        ans=[]
        result=0
        for i in a:
            result+=i
            ans.append(i)
            if result>(sum/2):
                return ans
